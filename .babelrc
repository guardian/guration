{
  "ignore": [
    "node_modules/**"
  ],
  "presets": [
    [
      "@babel/env",
      {
        "modules": false
      }
    ],
    "@babel/react",
    "@babel/flow"
  ],
  "plugins": [
    "@babel/proposal-class-properties"
  ],
  "env": {
    "test": {
      "presets": [
        "@babel/env",
        "@babel/react",
        "@babel/flow"
      ]
    }
  }
}
